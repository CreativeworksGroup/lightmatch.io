{
  "name": "lightmatch.io",
  "version": "1.0.0",
  "description":
    "The first ever photo sharing platform based on Creative Commons Zero in China.",
  "main": "config.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "pretest": "npm run lint --silent",
    "flow": "flow",
    "postinstall":
      "cd imports && mkdir uniforms && cd uniforms; git init; git remote add -f origin https://github.com/janowsiany/uniforms.git; git pull origin material-ui-next; npm i; cd ../..; npm i ./imports/uniforms/packages/uniforms-material/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CreativeworksGroup/lightmatch.io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CreativeworksGroup/lightmatch.io/issues"
  },
  "homepage": "https://github.com/CreativeworksGroup/lightmatch.io#readme",
  "dependencies": {
    "ali-oss": "^4.11.2",
    "bcrypt": "^1.0.3",
    "blueimp-load-image": "^2.17.1",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.21",
    "material-ui-icons": "^1.0.0-beta.17",
    "meteor-node-stubs": "^0.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-dropzone": "^4.2.3",
    "react-grid-layout": "^0.16.0",
    "react-image-lightbox": "^4.3.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.0.0-rc.10",
    "recompose": "^0.26.0",
    "simpl-schema": "^0.5.0",
    "uniforms": "^1.22.1",
    "uniforms-material": "file:imports/uniforms/packages/uniforms-material"
  },
  "devDependencies": {
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "babel-eslint": "^8.0.2",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-meteor": "^4.1.6",
    "eslint-plugin-react": "^7.5.0",
    "flow-bin": "^0.59.0",
    "prettier-eslint": "^8.2.2"
  },
  "eslintConfig": {
    "extends": [
      "@meteorjs/eslint-config-meteor",
      "plugin:flowtype/recommended"
    ],
    "plugins": ["flowtype"],
    "globals": {
      "_": false
    }
  },
  "babel": {
    "presets": ["flow"],
    "retainLines": true
  }
